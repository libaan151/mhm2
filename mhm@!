loadstring(game:HttpGet("https://raw.githubusercontent.com/libaan151/mhm/main/damn"))()


local keybind = "k"
--Change the e to whatever keybind you want just make sure its lower case



gv = false
    plr = game.Players.LocalPlayer
    mouse = plr:GetMouse()
    mouse.KeyDown:connect(function(key)
        if key == keybind and  gv == false then
            gv = true


            Aiming.Enabled = false

game.StarterGui:SetCore("SendNotification", {
    Title = "Silent Aim",
    Text = "Off",
    Duration = 0.7,
    Callback = bind,
})
        elseif key == keybind and gv == true then
            gv = false
            Aiming.Enabled = true
game.StarterGui:SetCore("SendNotification", {
    Title = "Silent Aim",
    Text = "On",
    Duration = 0.7,
    Callback = bind,
})
        end
    end)

    DaHoodSettings.AimLock = false
    Aiming.ShowFOV = false
    Aiming.FOV = 23
